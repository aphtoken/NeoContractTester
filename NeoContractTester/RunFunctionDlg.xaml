<Window x:Class="NeoContractTester.RunFunctionDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NeoContractTester"
        mc:Ignorable="d"
        Title="RunFunctionDialog" Height="386.863" Width="583.914" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="Parameters (Comma Separated)" HorizontalAlignment="Left" Margin="10,17,0,0" VerticalAlignment="Top"/>
        <Label Content="Result" HorizontalAlignment="Left" Margin="10,71,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="ResultTextBox" HorizontalAlignment="Left" Height="183" Margin="10,102,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="556" AcceptsReturn="True" AcceptsTab="True" IsEnabled="False"/>
        <Button x:Name="RunButton" Content="Run" HorizontalAlignment="Left" Margin="403,326,0,0" VerticalAlignment="Top" Width="75" Click="RunButton_Click"/>
        <Button x:Name="CanvelButton" Content="Cancel" HorizontalAlignment="Left" Margin="491,326,0,0" VerticalAlignment="Top" Width="75" Click="CanvelButton_Click"/>
        <TextBox x:Name="ParamValuesTextBox" HorizontalAlignment="Left" Height="23" Margin="10,43,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="556"/>

    </Grid>
</Window>
